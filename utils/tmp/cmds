docker build -t mysql-server:latest -f Dockerfile.mysql .

docker run --name mysql-server \
-e MYSQL_DATABASE=bookstorage \
-e MYSQL_ROOT_PASSWORD=root \
-e MYSQL_USERNAME_1=bookstorage_web_user \
-e MYSQL_USERNAME_2=bookstorage_db_user \
-e MYSQL_USERPASS_1=134679bookstorageWebUser! \
-e MYSQL_USERPASS_2=134679bookstorageDbUser! \
-d mysql-server:latest
